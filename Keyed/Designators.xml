  <!-- EN: Strip -->
  <DesignatorStrip>Röövi</DesignatorStrip>
  <!-- EN: Strip apparel from corpses and the wounded. -->
  <DesignatorStripDesc>Röövi rõivad laipadelt ja vigastatutelt.</DesignatorStripDesc>

  <!-- EN: Open -->
  <DesignatorOpen>Ava</DesignatorOpen>
  <!-- EN: Open containers. -->
  <DesignatorOpenDesc>Ava täis hauad või konteinerid.</DesignatorOpenDesc>
  
  <!-- EN: Slaughter -->
  <DesignatorSlaughter>Tapa</DesignatorSlaughter>
  <!-- EN: Slaughter a tame animal. -->
  <DesignatorSlaughterDesc>Tapa taltsutatud loom.</DesignatorSlaughterDesc>

  <!-- EN: Tame -->
  <DesignatorTame>Taltsuta</DesignatorTame>
  <!-- EN: Tame a wild animal. -->
  <DesignatorTameDesc>Talsuta metsik loom.</DesignatorTameDesc>

